# ls
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -F'

# grep
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

# Workpop
alias wpstart='meteor --settings tests/settings/local.json'
alias wp='cd /Users/jason/Programming/Workpop-Web'
alias wpv='wp; vim `wfind "*.js"` `wfind "*.html"`'
